DATE=2013-10-30

FREEZE=cgDataFreeze$DATE

python scriptRepoStats.py 

cp statReport data_flatten/statReport_$DATE
mv statReport CAVMold/statReport_$DATE

rsync -avL --exclude '*~' --exclude 'probeMap' CAVMold/* /data/cancerGenomeData/$FREEZE/

rsync -avL --exclude '*~' data_flatten/probeMap /data/cancerGenomeData/$FREEZE/

rsync -avL --exclude '*~' data_flatten/stat* /data/cancerGenomeData/$FREEZE/

#rsync -avL --exclude '*~' data_flatten/public/TCGA /data/cancerGenomeData/$FREEZE/public/

chmod -R a-wx+X /data/cancerGenomeData/$FREEZE/

chgrp -R cancer /data/cancerGenomeData/$FREEZE/

rm -f /data/cancerGenomeData/cgDataFreezeLatest 

ln -sf /data/cancerGenomeData/$FREEZE /data/cancerGenomeData/cgDataFreezeLatest